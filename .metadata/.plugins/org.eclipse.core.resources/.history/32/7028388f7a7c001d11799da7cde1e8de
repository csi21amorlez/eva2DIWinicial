package main.modelos.daos;

import java.util.ArrayList;


//Interfaz que define los metodos de EvaCatEvaluacion, su implementacion esta en EvaCatEvaluacionImpl
public interface EvaCatEvaluacionService {

	// Metodo para insertar registros en base de datos
	public void insertarRegistro(EvaCatEvaluacion dao);

	// Metodo para buscar todos los registros de la base de datos
	public ArrayList<EvaCatEvaluacion> buscarTodos();

}
